message IceAndFire
{
	optional int64 bet_amount = 1; //下注数额
    optional int64 free_spin_bouts = 2; //免费spin次数
    optional int64 bouts_id = 3;//局id,放用时间戳,服务端用于判定cd和日志
    optional string channel_id = 4;
    optional int64 total_loss = 5;
    optional int64 enter_chip = 6;
    optional int64 spined_times = 7;
    optional int64 free_total_win = 8;
	optional int64 free_item_id = 9;
    optional int64 bonus_game_type = 10;
    optional int64 mother_dragons_type = 11;//1只点击一次，2只点击二次，3只点击三次
    optional string mother_dragons_str = 12;//还剩的龙蛋
    optional string raven_prophet_str = 13;
    optional string json_str = 14;
}