import "Player.proto";

message Seat
{
	optional int64 id = 1;
	optional Player player = 2;
	optional int64 online = 3;
}

message Table
{
	repeated Seat seat = 1;
}

message Device
{
	optional int64 device_id = 1; // 设备号
	optional int64 player_id = 2; // 入座玩家id
	optional int64 lucky = 3; // 幸运值
	optional int64 win = 4; // 最近赢取值
	optional string room_name = 5; // 房间名 查询时使用
	optional string nickname = 6; //入座玩家昵称
	optional int64 sex = 7; // 入座玩家性別
}