import "Common.proto";
import "Player.proto";
import "CustomerService.proto";

message CustomerServicePasser_CustomerSay_Request
{
	optional Header header = 1;
	required Player player = 2;
	required CustomerServiceItem item = 3;
}

message CustomerServicePasser_CustomerSay_Response
{
	optional Header header = 1;
	required Return ret = 2;
}

message CustomerServicePasser_StaffSay_Request
{
	optional Header header = 1;
	required int64 player_id = 2;
	required CustomerServiceItem item = 3;
}

message CustomerServicePasser_StaffSay_Response
{
	optional Header header = 1;
	required Return ret = 2;
}