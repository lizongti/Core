import "Common.proto";
import "Player.proto";
import "Contest.proto";
import "Slots.proto";

message SlotsChefsChoice_Enter_Request
{
	optional Header header = 1;
	optional int32 table_id = 2;
}

message SlotsChefsChoice_Enter_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsChefsChoice_Start_Request
{
	optional Header header = 1;
	optional int64 amount = 2;
}

message SlotsChefsChoice_RemoveItem
{
	optional int64 row = 1;
	optional int64 column = 2;
	optional int64 item_id = 3;
}

message Slots_DropChefsChoiceCol
{
	repeated int64 item_ids = 1;
}

message Slots_DropChefsChoice
{
	repeated Slots_DropChefsChoiceCol drop_items = 1;
}


message SlotsChefsChoice_Start_Response
{
	optional Header header = 1;
	required Return ret = 2;
	repeated int64 item_ids = 3;
	optional Player player = 4;
	repeated Slots_Prize all_prize_items = 5;
	repeated Slots_DropChefsChoice all_drop_items = 6;
	repeated Slots_EraseItems all_erase_items = 7;
	repeated int64 win_chip = 8;
	optional int64 all_win_chip = 9;
	optional int64 is_multiply = 10;
	optional int64 free_spin_num = 11; //触发的freespin的类型5, 10, 20, 50
	optional int64 is_free_spin = 12;
}

message SlotsChefsChoice_Exit_Request
{
	optional Header header = 1;
}

message SlotsChefsChoice_Exit_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsChefsChoice_TableSync_Notice
{
	optional Header header = 1;
	optional Table table = 2;
}