import "Common.proto";
import "Player.proto";
import "Contest.proto";
import "Slots.proto";

message SlotsLuxuryLife_Enter_Request
{
	optional Header header = 1;
	optional int32 table_id = 2;
}

message SlotsLuxuryLife_Enter_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsLuxuryLife_Start_Request
{
	optional Header header = 1;
	optional int64 amount = 2;
}

message SlotsLuxuryLife_Start_Response
{
	optional Header header = 1;
	required Return ret = 2;
	repeated int64 item_ids = 3;
	optional Player player = 4;
	repeated Slots_Prize all_prize_items = 5;
	repeated string all_respin_items = 6;
	repeated int64 win_chip = 7;
	optional int64 all_win_chip = 8;
	optional int64 is_multiply = 9;
	repeated string freeze_list = 10;//锁住的元素位置列表
	optional int64 is_free_spin = 11;
}

message SlotsLuxuryLife_Wild_Request
{
	optional Header header = 1;
	optional int32 pos = 2;
}

message SlotsLuxuryLife_Wild_Response
{
	optional Header header = 1;
	required Return ret = 2;
}

message SlotsLuxuryLife_Exit_Request
{
	optional Header header = 1;
}

message SlotsLuxuryLife_Exit_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsLuxuryLife_TableSync_Notice
{
	optional Header header = 1;
	optional Table table = 2;
}