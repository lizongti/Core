}

message Account_ClientAction_Request
{
    optional Header header = 1;
	optional string content = 2;
}

message Account_ClientAction_Response
{
    optional Header header = 1;
    required Return ret = 2;
}

message Account_Config_Request
{
	optional Header header = 1;
	repeated string table_name = 2;
	required int64 game_type = 3;
}

message Account_Config_Response
{
    optional Header header = 1;
    required Return ret = 2;
	repeated string table_content = 3;
}

message Account_RateUs_Request
{
	optional Header header = 1;
}

message Account_RateUs_Response
{
    optional Header header = 1;
    required Return ret = 2;
}

message Account_PayFailAward_Request
{
	optional Header header = 1;
}

message Account_PayFailAward_Response
{
    optional Header header = 1;
    required Return ret = 2;
}

message Account_InWhiteList_Request
{
	optional Header header = 1;
}

message Account_InWhiteList_Response
{
    optional Header header = 1;
	required Return ret = 2;
	
	optional int64  is_in = 3;
}

message Account_Ping_Request
{
	optional Header header = 1;
}

message Account_Ping_Response
{
    optional Header header = 1;
    required Return ret = 2;
}

message Account_Cheat_Request
{
	optional Header header = 1;
	optional int64 type = 2;
