import "Common.proto";
import "Player.proto";
import "Contest.proto";
import "Slots.proto";

message SlotsChineseNewYear_Enter_Request
{
	optional Header header = 1;
	optional int32 table_id = 2;
}

message SlotsChineseNewYear_Enter_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsChineseNewYear_Start_Request
{
	optional Header header = 1;
	optional int64 amount = 2;
}

message SlotsChineseNewYear_Start_Response
{
	optional Header header = 1;
	required Return ret = 2;
	repeated int64 item_ids = 3;
	repeated Slots_PrizeItem prize_items = 4;
	optional int64 win_chip = 5;
	optional Player player = 6;
	optional int64 is_multiply = 7;
	optional int64 bonus_win_chip = 8;
	optional int64 is_free_spin = 9;
}

message SlotsChineseNewYear_Bonus_Request
{
	optional Header header = 1;
	optional int64 is_bonus_bet = 2;
}

message SlotsChineseNewYear_Bonus_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsChineseNewYear_SelFreeStyle_Request
{
	optional Header header = 1;
	optional int64 free_spin_type = 2;
}

message SlotsChineseNewYear_SelFreeStyle_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsChineseNewYear_Exit_Request
{
	optional Header header = 1;
}

message SlotsChineseNewYear_Exit_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
}

message SlotsChineseNewYear_TableSync_Notice
{
	optional Header header = 1;
	optional Table table = 2;
}