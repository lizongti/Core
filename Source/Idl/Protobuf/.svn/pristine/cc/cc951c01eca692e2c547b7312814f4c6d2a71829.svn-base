message Slots_PrizeItem
{
	optional int64 continue_count = 1;
	optional int64 item_id = 2;
	optional int64 line_index = 3;
	optional int64 payrate = 4;
	optional int64 extra_paid = 5;//吸血鬼玩法中用到,用以标记这一线赔率是否乘以了2,是的话为1,否则为0
	optional int64 from_index = 6;
	optional int64 to_index = 7;
	optional string pos_list = 8;//中奖的位置列表
	optional int64 line_count = 9;//reel ways连线的连线数
}

message Slots_Guidance_DataItem
{
	repeated int64 item_ids = 1;
	repeated int64 wilds = 2;
	repeated Slots_PrizeItem prize_items = 3;
	optional int64 win_chip = 4;
	optional int64 has_bonus = 5;//本次spin是否产生了bonus+1,0:false, 1:true
	optional int64 is_free_spin = 6;//本次是否是free spin, 0:false, 1:true
}

message Slots_Item
{
	optional int64 row = 1;
	optional int64 col = 2;
}

message Slots_EraseItems
{
	repeated Slots_Item erase_items = 1;
}

message Slots_DropFruitCol
{
	repeated int64 item_ids = 1;
}

message Slots_DropFruit
{
	repeated Slots_DropFruitCol drop_items = 1;
}

message Slots_Prize
{
	repeated Slots_PrizeItem prize_items = 1;
}

//fruit slice玩法用到
message Slots_FruitExplode
{
    optional int64 exp_fruit_id = 1;
    optional int64 player_id = 2;
    optional int64 win_chip = 3;
}

//pharaoh treasure玩法用到
message PickHistory
{
    optional int64 pick_index = 1;//小游戏选择的索引,从左到右,从1数起
    optional int64 win_chip = 2;//pick得到的筹码数,如果这个筹码数为0,则表示pick到了upstairs
}