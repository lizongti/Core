import "Club.proto";
import "Common.proto";
import "Player.proto";

//create club
message Club_Create_Request
{
	optional Header header = 1;
	optional Club club = 2;
}

message Club_Create_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
	optional Club club = 4;
    optional int64 join_bonus = 5;
}

//query club info
message Club_Query_Request
{
	optional Header header = 1;
	optional ClubFilter filter = 2;//查询的过滤器
	optional string filter_name = 3;//以名字查询
}

message Club_Query_Response
{
	optional Header header = 1;
	required Return ret = 2;
	repeated Club clubs = 3;
}

//查看俱乐部申请和邀请的请求
message Club_QueryRequests_Request
{
	optional Header header = 1;
}

message Club_QueryRequests_Response
{
	optional Header header = 1;
	required Return ret = 2;
	repeated ClubRequest requests = 3;
	repeated ClubInvitation invitations = 4;
}

//接收邀请的请求
message Club_AcceptInvitation_Request
{
	optional Header header = 1;
	optional int64 invite_id = 2;
}

message Club_AcceptInvitation_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
    optional int64 join_bonus = 4;
}

//拒绝邀请
message Club_DeclineInvitation_Request
{
	optional Header header = 1;
	optional int64 invite_id = 2;
}

message Club_DeclineInvitation_Response
{
	optional Header header = 1;
	required Return ret = 2;
}

//通过申请
message Club_ApproveRequest_Request
{
	optional Header header = 1;
	optional int64 request_id = 2;
}

message Club_ApproveRequest_Response
{
	optional Header header = 1;
	required Return ret = 2;
}

//拒绝申请
message Club_RejectRequest_Request
{
	optional Header header = 1;
	optional int64 request_id = 2;	
}

message Club_RejectRequest_Response
{
	optional Header header = 1;
	required Return ret = 2;
}

//加入俱乐部的请求
message Club_Join_Request
{
	optional Header header = 1;
	optional int64 club_id = 2;
}

message Club_Join_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Club club = 3;
	optional Player player = 4;
    optional int64 join_bonus = 5;
}

message Club_Apply_Request
{
	optional Header header = 1;
	optional int64 club_id = 2;	
}

message Club_Apply_Response
{
	optional Header header = 1;
	required Return ret = 2;	
}

//给俱乐部捐赠的请求
message Club_Fund_Request
{
	optional Header header = 1;
	optional int64 fund_amount = 2;
}

message Club_Fund_Response
{
	optional Header header = 1;
	required Return ret = 2;
	optional Player player = 3;
	optional Club club = 4;
}

//邀请别的玩家加入俱乐部的请求
message Club_Invite_Request
{
	optional Header header = 1;
	optional int64 player_id = 2;
}

message Club_Invite_Response
{
	optional Header header = 1;
	required Return ret = 2;
}

